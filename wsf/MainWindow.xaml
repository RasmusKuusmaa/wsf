<Window x:Class="wsf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wsf"
        xmlns:vm="clr-namespace:wsf.ViewModels"
        mc:Ignorable="d"
        Title="wsf" Height="450" Width="800">
    <Window.DataContext>
        <vm:MainViewModel/>
    </Window.DataContext>
    <StackPanel Margin="10">
        <CheckBox IsChecked="{Binding IsBingEnabled}" Content="Bing enabled" />
    </StackPanel>
</Window>
